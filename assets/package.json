{
  "name": "@solidworx/platform",
  "description": "SolidWorx Platform UI scripts",
  "license": "MIT",
  "version": "1.0.0",
  "type": "module",
  "files": [
    "controllers"
  ],
  "keywords": ["symfony-ux"],
  "symfony": {
    "controllers": {
      "modal": {
        "main": "controllers/modal.js",
        "webpackMode": "lazy",
        "fetch": "lazy",
        "enabled": true
      },
      "csrf-protection": {
        "main": "controllers/csrf_protection.js",
        "webpackMode": "lazy",
        "fetch": "lazy",
        "enabled": true
      },
      "loading": {
        "main": "controllers/loading.js",
        "webpackMode": "lazy",
        "fetch": "lazy",
        "enabled": true
      }
    },
    "importmap": {
      "@hotwired/stimulus": "^3.2.2",
      "@symfony/stimulus-bridge": "^4.0.1",
      "@tabler/core": "^1.4.0",
      "bootstrap": "^5.3.8"
    }
  },
  "peerDependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@symfony/stimulus-bridge": "^4.0.1",
    "@tabler/core": "^1.4.0",
    "bootstrap": "^5.3.8"
  },
  "dependencies": {
    "@hotwired/stimulus": "^3.2.2",
    "@symfony/stimulus-bridge": "^4.0.1",
    "@tabler/core": "^1.4.0",
    "bootstrap": "^5.3.8"
  },
  "peerDependenciesMeta": {
    "@tabler/core": {
      "optional": false
    },
    "bootstrap": {
      "optional": false
    },
    "@hotwired/stimulus": {
      "optional": false
    },
    "@symfony/stimulus-bridge": {
        "optional": false
